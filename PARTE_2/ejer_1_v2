def fibonacci(n):
    fib_sequence = [0, 1]
    for i in range(2, n):
        next_number = fib_sequence[i-1] + fib_sequence[i-2]
        fib_sequence.append(next_number)
    return fib_sequence

# Ejemplo de uso
n = 10
resultado = fibonacci(n)

# Convertimos la lista a una cadena de n√∫meros separados por comas
print(", ".join(map(str, resultado)))
